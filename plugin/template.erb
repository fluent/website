    <title>Fluentd plugins</title>
    <link rel="stylesheet" href="../doc/_static/bizstyle.css" type="text/css" />
    <link rel="stylesheet" href="../doc/_static/pygments.css" type="text/css" />
    <script type="text/javascript">
      var DOCUMENTATION_OPTIONS = {
        URL_ROOT:    '#',
        VERSION:     '0.10.0',
        COLLAPSE_MODINDEX: false,
        FILE_SUFFIX: '.html',
        HAS_SOURCE:  true
      };
    </script>
    <script type="text/javascript" src="../doc/_static/jquery.js"></script>
    <script type="text/javascript" src="../doc/_static/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="../doc/_static/doctools.js"></script>
    <style type="text/css">
      div.body h2 {
        margin-top: 0.2em;
        line-height: 1.3em;
        font-size: 140%;
        border: none;
      }
      div.body a:hover {
        text-decoration: underline;
      }
      tr.even { background-color: #f0f0f0; } 
    </style>
    <script>
      $(document).ready(function() {
        $('#plugin-table').dataTable({
          "bPaginate": false,
          "aaSorting": [[4, "desc"]],
          "aoColumns": [null, null, null, null, null]
        });
        $('#plugin-table_filter input').attr('placeholder',
          'search for mongo, redis, etc...').css('width', '250px');
      });
    </script>
  </head>
  <body>
    <div class="related">
      <ul>
        <li><a href="../doc/">fluentd documentation</a> &raquo;</li> 
      </ul>
    </div>
    <div class="document">
      <div class="documentwrapper">
        <div class="bodywrapper">
          <div class="body">
            <h2>Fluent Plugins</h2>
            <table id="plugin-table">
              <thead>
                <tr>
                  <th>Plugin Name</th>
                  <th>gem name &amp; version</th>
                  <th>Author</th>
                  <th>About</th>
                  <th># of Downloads</th>
                </tr>
              </thead>
              <tbody>
              <% plugins.each { |pl| %>
                <tr>
                  <td><a href="<%=pl.url%>"><%=pl.name%></a></td>
                  <td><a href="<%=pl.url%>"><%=pl.gemname+' '+pl.version%></a></td>
                  <td><%=pl.author%></td>
                  <td><%=pl.summary%></td>
                  <td><%=pl.downloads%></td>
                </tr>
              <% } %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="clearer"></div>
    </div>

